<h1>Add a Demand</h1>
<%= simple_form_for @demand, html: { data: { demands_index_target: 'form' }} do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :category %>
  <%= f.input :start_date, as: :date, html5: true %>
  <%= f.input :end_date, as: :date, html5: true %>
  <%= f.button :submit do %>
    <i class="far fa-save"></i>
  <% end %>
<% end %>
